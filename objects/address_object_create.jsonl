{
  "id": "address_object_create",
  "content": {
    "title": "Create Address Object in PAN-OS (v11.0+)",
    "category": "Object Management â†’ Address Objects",
    "api_endpoint": "POST /restapi/v11.0/Objects/Addresses?location=<location>&vsys=<vsys>&name=<object-name>",
    "summary": "Creates a new address object under a specific location such as a vsys. The object must include a valid name and define either an FQDN, IP address, or IP range. In PAN-OS v11.0 and later, @location is optional inside the body, but the 'name' query parameter must match @name inside the entry.",
    "required_query_parameters": {
      "location": "Specifies scope such as 'shared' or 'vsys'.",
      "vsys": "Required if location is 'vsys', e.g., vsys1.",
      "name": "Name of the address object being created. Must match @name in the body."
    },
    "request_body_format": {
      "entry": [
        {
          "@name": "<object-name>",
          "fqdn | ip-netmask | ip-range": "<value>",
          "description": "<optional>",
          "tag": {
            "member": [
              "tag1"
            ]
          }
        }
      ]
    },
    "example_request": "curl -k -X POST 'https://174.92.110.3/restapi/v11.0/Objects/Addresses?location=vsys&vsys=vsys1&name=web-servers-production' -H 'X-PAN-KEY: LUFRPT1hbUIvc1NrbnV4VEV1dlJLVDVuWWlDRi9vdXc9cml5NkFOSU9wOG5yTlZvUTVod3FGYWtvVXV0ajBWWkpoZXJKb2xFMzU3TllnS1RHNXpuUnFzd2IxZ2I5R1o0Sg==' -d '{"entry": [{"@location": "vsys","@name": "web-servers-production","description": "what is this for?","fqdn": "docs.paloaltonetworks.com"}]}'",
    "expected_success_response": {
      "@code": "19",
      "@status": "success",
      "msg": "command succeeded"
    },
    "common_errors": [
      "400: Missing required query parameters (location, vsys, or name)",
      "403: Invalid or unauthorized API key",
      "409: An object with the same name already exists"
    ],
    "version_support": "PAN-OS 11.0 and later (this format)",
    "related_operations": [
      "Edit: PUT /restapi/v11.0/Objects/Addresses?location=<loc>&vsys=<vsys>&name=<name>",
      "Rename: POST /restapi/v11.0/Objects/Addresses:rename?location=<loc>&vsys=<vsys>&name=<old>&newname=<new>",
      "Delete: DELETE /restapi/v11.0/Objects/Addresses?location=<loc>&vsys=<vsys>&name=<name>",
      "Get/List: GET /restapi/v11.0/Objects/Addresses?location=<loc>&vsys=<vsys>"
    ]
  },
  "metadata": {
    "category": "Address Objects",
    "operation": "Create",
    "endpoint": "/restapi/v11.0/Objects/Addresses",
